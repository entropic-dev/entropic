{
  "name": "entropic",
  "description": "community package manager",
  "version": "0.0.1",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "bugs": {
    "url": "https://github.com/ceejbot/entropic/issues"
  },
  "dependencies": {
    "@hapi/iron": "^5.1.0",
    "@hapi/joi": "^15.0.0",
    "cls-hooked": "^4.2.2",
    "cookie": "^0.3.1",
    "csrf": "~3.1.0",
    "culture-ships": "~1.0.0",
    "dotenv": "^7.0.0",
    "escape-html": "^1.0.3",
    "find-my-way": "~2.0.1",
    "is-email-maybe": "^1.0.1",
    "micro": "~9.3.3",
    "mkdirp": "~0.5.1",
    "multiparty": "^4.2.1",
    "node-fetch": "^2.4.1",
    "npm-user-validate": "^1.0.0",
    "ormnomnom": "^5.2.2",
    "pacote": "~9.5.0",
    "pg": "~7.4.0",
    "pino": "^5.12.3",
    "redis": "^2.8.0",
    "semver": "^6.0.0",
    "ssri": "^6.0.1",
    "uuid": "~3.3.2"
  },
  "devDependencies": {
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.5.0",
    "eslint": "~5.16.0",
    "eslint-config-prettier": "~4.2.0",
    "eslint-plugin-prettier": "~3.0.1",
    "mocha": "~6.1.4",
    "must": "~0.13.4",
    "nyc": "~14.0.0",
    "pgtools": "^0.3.0",
    "pino-pretty": "~3.0.0",
    "prettier": "1.17.0",
    "test-listen": "^1.1.0"
  },
  "homepage": "https://github.com/ceejbot/entropic#readme",
  "keywords": [
    "entropy",
    "heat death"
  ],
  "license": "Apache-2.0",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/ceejbot/entropic.git"
  },
  "scripts": {
    "coverage": "nyc mocha -R spec",
    "createdb": "createdbjs -U postgres entropic_${NODE_ENV:-dev} || true",
    "lint": "prettier -c --single-quote '**/*.js'",
    "lint-fix": "prettier --single-quote --write '**/*.js'",
    "migrate": "PGUSER=postgres PGDATABASE=entropic_${NODE_ENV:-dev} db-migrate up --config db-migrate.json --env entropic",
    "posttest": "true || npm run lint",
    "preregistry": "NODE_ENV=dev npm run createdb >/dev/null; NODE_ENV=dev npm run migrate",
    "pretest": "NODE_ENV=test npm run createdb >/dev/null; NODE_ENV=test npm run migrate",
    "registry": "PGUSER=postgres PGDATABASE=entropic_${NODE_ENV:-dev} ./registry/server.js",
    "test": "PGUSER=postgres PGDATABASE=entropic_test mocha -R spec"
  }
}
